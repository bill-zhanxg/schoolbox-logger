{
	"name": "schoolbox-logger",
	"private": true,
	"scripts": {
		"dev": "turbo run dev",
		"build": "turbo run build --affected",
		"start": "turbo run start",
		"lint": "turbo run lint",
		"prettier": "prettier --write '**/*.{ts,tsx,css,json,sql}'",
		"prisma": "npm run prisma -w @repo/database",
		"prisma:generateSql": "npm run prisma:generateSql -w @repo/database"
	},
	"dependencies": {
		"@prisma/client": "6.1.0",
		"database": "^0.0.2",
		"dotenv-cli": "^8.0.0",
		"prettier-plugin-sql-cst": "^0.12.0",
		"prettier-plugin-tailwindcss": "^0.6.9"
	},
	"devDependencies": {
		"prettier": "^3.4.2",
		"prisma": "6.1.0",
		"turbo": "^2.3.3"
	},
	"engines": {
		"node": ">=20"
	},
	"prisma": {
		"schema": "packages/database/prisma/schema.prisma"
	},
	"packageManager": "npm@11.0.0",
	"workspaces": [
		"apps/*",
		"packages/*"
	]
}
