{
	"$schema": "https://turbo.build/schema.json",
	"tasks": {
		"dev": {
			"cache": false,
			"persistent": true
		},
		"build": {
			"dependsOn": ["^build"],
			"inputs": ["$TURBO_DEFAULT$", ".env*"],
			"outputs": ["dist/**", ".next/**", "!.next/cache/**"]
		},
		"start": {},
		"prisma:migrate:dev": {
			"cache": false,
			"interactive": true
		},
		"prisma:generateSql": {
			"cache": false
		},
		"lint": {}
	},
	"ui": "stream"
}
