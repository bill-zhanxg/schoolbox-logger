{
	"name": "schoolbox-logger-backend",
	"version": "0.0.0",
	"description": "Log any Schoolbox instance such as users, and provide better search filters with Azure integration",
	"main": "./dist/index.js",
	"scripts": {
		"dev": "dotenv -e ../../.env -c development -- bun --watch --env-file=.env.development.local run src/index.ts",
		"build": "dotenv -e ../../.env -c production npm run clean && tsc",
		"start": "dotenv -e ../../.env -c production -- bun --env-file=.env.production.local run dist/index.js",
		"clean": "rimraf dist",
		"pull": "xata pull main",
		"pull:dev": "xata pull dev"
	},
	"keywords": [],
	"author": "Bill.IHCha",
	"license": "MIT",
	"dependencies": {
		"@aws-sdk/client-s3": "^3.717.0",
		"@aws-sdk/lib-storage": "^3.717.0",
		"@microsoft/microsoft-graph-client": "^3.0.7",
		"@repo/database": "*",
		"@xata.io/client": "^0.30.1",
		"bun": "^1.1.42",
		"cheerio": "^1.0.0",
		"express": "^4.21.2",
		"queue": "^7.0.0",
		"tslib": "^2.8.1",
		"uuid": "^11.0.3"
	},
	"devDependencies": {
		"@microsoft/microsoft-graph-types": "^2.40.0",
		"@types/express": "^5.0.0",
		"@types/node": "^22.10.2",
		"rimraf": "^6.0.1",
		"ts-node-dev": "^2.0.0",
		"typescript": "^5.7.2"
	},
	"engines": {
		"node": ">=20.0.0"
	}
}
